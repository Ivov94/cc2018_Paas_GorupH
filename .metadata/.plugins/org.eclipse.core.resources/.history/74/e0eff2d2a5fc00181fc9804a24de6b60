package paas.mongodb;

import java.util.Date;
import org.springframework.data.annotation.Id;

public class Image {
	
	@Id
	public String id;
	
	public String name;
	public int data;
	public String mimeType;
	public String filename;
	public Date date;
	public int step;
	public boolean isDone;
	
	public Image()
	{
	}
	
	
}
